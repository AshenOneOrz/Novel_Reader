{
    "name": "readnovel",
    "displayName": "readNovel",
    "description": "",
    "version": "0.9.9",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:readnovel.helloWorld"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "readnovel.helloWorld",
                "title": "Hello World"
            }
        ],
        "configuration": {
            "title": "ReadNovel",
            "type": "object",
            "properties": {
                "readNovel.filePath": {
                    "type": "string",
                    "default": "",
                    "description": "path"
                },
                "readNovel.annotationRule": {
                    "type": "string",
                    "default": "//$",
                    "suggestions": "xx",
                    "description": "rule"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.61.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "glob": "^7.1.7",
        "mocha": "^9.1.1",
        "typescript": "^4.4.3",
        "@vscode/test-electron": "^1.6.2"
    }
}
